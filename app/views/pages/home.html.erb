	<% provide(:title, 'Home') %>
<div class="jumbotron center">
	<h1>Welcome to my app!</h1>
	<p>
		<% if user_signed_in? %>
			<p>Welcome Back!</p>
		<% else %>
			<p>
			<%= link_to "Login", new_user_session_path, class: "btn btn-default btn-lg" %>
			<%= link_to "Sign Up", new_user_registration_path, class: "btn btn-primary btn-lg" %>
			</p>
		<% end %>
		
	</p>
</div>